  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Identity.Core" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Volo\Abp\Permissions\" />
  </ItemGroup>

</Project>
