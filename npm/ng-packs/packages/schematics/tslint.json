{
  "extends": "../../tslint.json",
  "linterOptions": {
    "exclude": ["**/utils/angular/**/*"]
  },
  "rules": {
    "no-non-null-assertion": false,
    "no-string-literal": false,
    "variable-name": {
      "options": ["allow-leading-underscore", "ban-keywords", "check-format", "allow-pascal-case"]
    }
  }
}
