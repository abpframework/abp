    <ProjectReference Include="..\Volo.Abp.Security\Volo.Abp.Security.csproj" />
    <ProjectReference Include="..\Volo.Abp.Threading\Volo.Abp.Threading.csproj" />
    <ProjectReference Include="..\Volo.Abp.Timing\Volo.Abp.Timing.csproj" />
  </ItemGroup>

